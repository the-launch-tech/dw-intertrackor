{
  "name": "dw-intertrackor-client",
  "version": "1.0.0",
  "description": "Client for Intertrackor App",
  "main": "dist/index.js",
  "author": "Daniel Griffiths",
  "license": "ISC",
  "scripts": {
    "tsc": "tsc",
    "dev": "export NODE_ENV=development && npm-run-all --parallel d:*",
    "d:tsc": "tsc --watch --preserveWatchOutput",
    "d:server": "webpack --config webpack.server.config.js --watch",
    "d:client": "webpack --config webpack.client.config.js --watch",
    "d:start": "nodemon --watch dist --exec \"node dist/server.js\"",
    "prod:build": "export NODE_ENV=production && npm-run-all --parallel p:*",
    "p:tsc": "tsc",
    "p:server": "webpack --config webpack.server.config.prod.js",
    "p:client": "webpack --config webpack.client.config.prod.js",
    "prod:start": "nodemon --watch dist --exec \"node dist/server.js\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "d3": "^5.16.0",
    "d3-array": "^2.4.0",
    "d3-axis": "^1.0.12",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "serialize-javascript": "^3.1.0",
    "three": "^0.118.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@types/d3": "^5.7.2",
    "@types/express": "^4.17.6",
    "@types/moment": "^2.13.0",
