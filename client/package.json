    "@types/react-router-dom": "^5.1.5",
    "@types/serialize-javascript": "^1.5.0",
    "@types/three": "^0.103.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.4",
